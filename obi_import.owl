    <!-- http://purl.obolibrary.org/obo/OBI_0000434 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000434">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000686"/>
